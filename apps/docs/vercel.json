{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        {
            "source": "/api-docs/:match*",
            "destination": "https://guilded-js.gitbook.io/api-docs/:match*"
        },
        {
            "source": "/guide/:match*",
            "destination": "https://guilded-js.gitbook.io/api-docs/:match*"
        },
        {
            "source": "/legacy/:match*",
            "destination": "https://guildedjs.github.io/:match*"
        }
    ]
}
