{
    "name": "guilded.js",
    "version": "0.0.1-dev",
    "description": "A Node.js library for the Guilded.gg (https://www.guilded.gg/) API written in TypeScript, usable in either JavaScript or TypeScript projects.",
    "types": "types/index.d.ts",
    "main": "dist/index.js",
    "author": "Zaid \"Nico\"",
    "license": "MIT",
    "scripts": {
        "test": "cd ../../ && yarn build && cd packages/guilded.js && node ./__tests__/index.js",
        "test:local": "yarn build && cd __tests__ && node index.js",
        "build": "tsc",
        "prepublishOnly": "npx rimraf dist/ && npx rimraf types/ && yarn build"
    },
    "devDependencies": {
        "@guildedjs/guilded-api-typings": "^2.0.0",
        "@types/node": "^14.14.9",
        "typescript": "^4.4.2"
    },
    "dependencies": {
        "@discordjs/collection": "^0.1.6",
        "@guildedjs/rest": "^2.0.0",
        "@guildedjs/ws": "^0.0.1"
    },
    "files": [
        "dist",
        "types"
    ],
    "keywords": [
        "guildedjs",
        "guilded.js",
        "guilded.gg",
        "guilded-api",
        "guilded"
    ],
    "directories": {
        "lib": "lib",
        "test": "__tests__"
    },
    "homepage": "https://github.com/guildedjs/guilded.js/tree/main/packages/guilded.js#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/guildedjs/guilded.js.git"
    },
    "bugs": {
        "url": "https://github.com/guildedjs/guilded.js/issues"
    },
    "gitHead": "0719b880becce0aa4bccaf5eb5b210ecb08b0547"
}
